# Copie este arquivo para .env e ajuste com suas credenciais reais

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=sua_access_key_aqui
MINIO_SECRET_KEY=sua_secret_key_aqui
MINIO_SECURE=False
MINIO_BUCKET=tecnomyl

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000

# Authentication (Single User)
AUTH_USERNAME=admin
AUTH_PASSWORD=sua_senha_aqui
JWT_SECRET_KEY=sua_chave_secreta_jwt_aqui_muito_longa_e_segura
JWT_ALGORITHM=HS256
JWT_EXPIRE_HOURS=24 